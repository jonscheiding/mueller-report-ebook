{
  "name": "nyt-mueller-scraper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec yarn run serve",
    "build": "yarn run build:epub",
    "build:epub": "babel-node index.js",
    "build:unzip": "unzip -o -d build/output/ build/output.epub",
    "serve": "yarn run build:epub && yarn run build:unzip && serve -s build/output/OEBPS/",
    "open": "yarn run build:epub && open build/output.epub"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "cheerio": "^1.0.0-rc.3",
    "epub-gen": "^0.1.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "nodemon": "^1.18.11",
    "serve": "^11.0.0"
  }
}
