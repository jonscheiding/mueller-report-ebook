{
  "name": "nyt-mueller-scraper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec yarn run serve",
    "dev:generate": "babel-node index.js",
    "dev:unzip": "unzip -o -d output output/output.epub",
    "dev:serve": "serve -s output/OEBPS/",
    "dev:open": "open output/output.epub",
    "serve": "yarn run dev:generate && yarn run dev:unzip && yarn run dev:serve",
    "open": "yarn run dev:generate && open output/output.epub"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "cheerio": "^1.0.0-rc.3",
    "epub-gen": "^0.1.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "serve": "^11.0.0",
    "shortid": "^2.2.14"
  }
}
